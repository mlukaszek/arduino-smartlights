        },
        
        "parter.pomaranczowy": "toggle:parter.salon.sufit",
        
        "parter.naroznik": {
            "short"  : "toggle:parter.salon.podwieszany",
            "medium" : "toggle:parter.salon.sufit"
        },
        
        "parter.hol"                   : "toggle:parter.hol.sufit",
        "parter.kuchnia.ekspres"       : "toggle:parter.ekspres",
        "parter.kuchnia.scianka.lewy"  : "toggle:parter.jadalnia",
        "parter.kuchnia.scianka.prawy" : "toggle:parter.kuchnia.blat",
        "parter.kuchnia.okap.lewy"     : "toggle:parter.kuchnia.podwieszany",
        "parter.kuchnia.okap.prawy"    : "toggle:parter.kuchnia.blat",
        
        "parter.gabinet"    : "toggle:parter.gabinet.sufit",
        "parter.sypialnia"  : "toggle:parter.sypialnia.sufit",
        "parter.lazienka"   : "toggle:parter.lazienka",
        
        "parter.przedsionek" : {
            "short"  : "toggle:parter.przedsionek",
            "medium" : "toggle:parter.hol.sufit",
            "long"   : "off"
        },
        
        "parter.wc" : "toggle:parter.wc",
        
        "piwnica.zejscie.gora"     : "toggle:piwnica.spocznik",
        "piwnica.zejscie.dol"      : "toggle:piwnica.schody",
        "piwnica.wejscie.lewy"     : "toggle:piwnica.hol",
        "piwnica.wejscie.srodkowy" : "toggle:piwnica.schody",
        "piwnica.wejscie.prawy"    : "toggle:piwnica.spocznik",
        "piwnica.warsztat"         : "toggle:piwnica.warsztat",
        
        "piwnica.sala.pojedynczy"     : "toggle:piwnica.sala.komin",
        "piwnica.sala.podwojny.lewy"  : "toggle:piwnica.sala.srodek",
        "piwnica.sala.podwojny.prawy" : {
            "short"  : "toggle:piwnica.sala.boki",
            "medium" : "toggle:piwnica.sala.kanapa"
        },
        
        "piwnica.kotlownia.lewy"  : "toggle:piwnica.kotlownia.kociol",
        "piwnica.kotlownia.prawy" : "toggle:piwnica.kotlownia.regaly"
